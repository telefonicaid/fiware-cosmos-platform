{
	vector TwoInt hours;
}
Data ClusterVector
{
	vector Double coms;
}
Data Cluster
{
	uint8 label;
	uint8 labelgroup;
	uint8 confident;	# 0: No confident	1: Confident
	double mean;
	double distance;
	ClusterVector coords;
}
Data ClusterSet
{
	vector Cluster cluster;
}

