<?xml version="1.0"?>
<?xml-stylesheet type="text/xsl" href="configuration.xsl"?>
<configuration>
    <property>
        <name>kpifilters</name>
        <value>
            es.tid.cosmos.kpicalculation.cleaning.ExtensionFilter,
            es.tid.cosmos.kpicalculation.cleaning.ThirdPartyFilter,
            es.tid.cosmos.kpicalculation.cleaning.PersonalInfoFilter
        </value>
        <description>Definition of filters applied</description>
    </property>
    <property>
        <name>kpifilters.extension</name>
        <value>
            js
            jpg
            jpeg
            css
            gif
            png
            bmp
            ico
            tif
            tiff
        </value>
        <description>
            Forbidden extensions for ExtensionFilter class
        </description>
    </property>
    <property>
        <name>kpifilters.3rdparty</name>
        <value>
            sexsearch.com
            admeld.com
            doubleclick.net
            adserverplus.com
            smartadserver.com
        </value>
        <description>
            Forbidden 3rd Party domains
        </description>
    </property>
    <property>
        <name>kpifilters.personalinfo</name>
        <value>
            pornhub.com
        </value>
        <description>
            Forbidden Personal Info domains
        </description>
    </property>
</configuration>
